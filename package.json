{
  "name": "@spritejs/next",
  "version": "0.1.5",
  "description": "The next generation of spritejs.",
  "main": "dist/spritejs.js",
  "module": "lib/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "start": "webpack-dev-server --env.server=examples --watch-poll",
    "compile": "rm -rf lib/* && babel src -d lib",
    "doc": "docsify serve ./docs",
    "build:debug": "webpack --env.mode=none",
    "build": "webpack --env.mode=production",
    "demo:dev": "demosify --serve",
    "demo:prod": "demosify --prod",
    "prepublishOnly": "npm run compile && npm run build && npm run demo:prod"
  },
  "keywords": [],
  "author": "akira-cn",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.6.2",
    "@demosify/core": "^0.5.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-gl-matrix": "^0.6.0",
    "canvas": "^2.6.0",
    "colors": "^1.4.0",
    "coveralls": "^3.0.8",
    "docsify": "^4.9.4",
    "eslint": "^5.16.0",
    "eslint-config-sprite": "^1.0.6",
    "eslint-plugin-html": "^5.0.5",
    "gl": "^4.4.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "raw-loader": "^3.1.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@mesh.js/core": "^0.6.6",
    "color-rgba": "^2.1.1",
    "css-select": "^2.0.2",
    "gl-matrix": "^3.1.0",
    "pasition": "^1.0.2",
    "sprite-animator": "^1.11.4"
  },
  "jest": {
    "testRegex": "/test/.*.test.js",
    "setupFiles": [
      "jest-canvas-mock"
    ]
  }
}
